        <record id="view_project_form_inherit_timesheet_button" model="ir.ui.view">
            <field name="name">project.project.form.inherit.timesheet.button</field>
            <field name="model">project.project</field>
            <field name="inherit_id" ref="project.edit_project"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@name='button_box']" position="inside">
                    <button name="action_view_timesheets"
                            type="object"
                            class="oe_stat_button"
                            icon="fa-clock-o">
                        <div class="o_field_widget o_stat_info">
                            <span class="o_stat_value">
                                <!-- Use float_time widget for proper time display -->
                                <field name="timesheet_count" widget="float_time"
                                       options="{'display_format': 'hh:mm'}"/>
                            </span>
                            <span class="o_stat_text">Hours Spent</span>
                        </div>
                    </button>
                </xpath>
            </field>
        </record>

        <record id="view_project_form_inherit_custom" model="ir.ui.view">
            <field name="name">project.project.form.inherit.custom.fields</field>
            <field name="model">project.project</field>
            <field name="inherit_id" ref="project.edit_project"/>
            <field name="arch" type="xml">


                <xpath expr="//field[@name='tag_ids']" position="after">

                    <!--                    <field name="account_id"/>-->
                    <field name="architect_id"/>
                    <field name="ba_id"/>
                    <field name="pm_id"/>
<!--                    <field name="status"/>-->
                    <field name="sync_wc"/>
                    <field name="wc_id"/>
                    <!--                    <field name="finance_description"/>-->


                </xpath>

                <xpath expr="//field[@name='user_id']" position="after">

                    <field name="brd_approval_date"/>
                    <field name="brd_submission_date"/>
                    <field name="go_live_date"/>
                    <!--                    <field name="end_date"/>-->
                    <field name="kick_start_meeting_date"/>
                    <field name="sandbox_review_date"/>
                    <!--                    <field name="start_date"/>-->
                    <field name="support_start_date"/>
                    <field name="uat_start_date"/>
                    <field name="warranty_end_date"/>


                </xpath>

                <xpath expr="//page[@name='description']" position="before">
                    <page name="extra_info" string="Extra Information">
                        <!--                        <field name="description"/>-->
                        <group>
                            <group>
                            <field name="payment_terms_id"/>
                            <field name="user_name"/>
                            <field name="password"/>
                            <field name="poc_email"/>
                            <field name="poc_mobile"/>
                            <field name="short_code"/>
                            <field name="comments"/>
                            <field name="development"/>
                        </group>
                        <group>
                            <field name="hourly_billing_rate"/>
                            <field name="hourly_cost"/>
                            <field name="hours_balance"/>
                            <field name="hours_est_pm"/>
                            <field name="hours_est_qa"/>
                            <field name="hours_est_dev"/>
                            <field name="hours_overflowed"/>
                            <field name="stories"/>

                        </group>

                        </group>


                    </page>
                </xpath>
            </field>
        </record>

