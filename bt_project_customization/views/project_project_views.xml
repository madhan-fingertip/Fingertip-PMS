<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>


        <record id="view_hr_timesheet_line_tree_inherit" model="ir.ui.view">
            <field name="name">hr.timesheet.line.tree.inherit</field>
            <field name="model">account.analytic.line</field>
            <field name="inherit_id" ref="hr_timesheet.timesheet_view_tree_user"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='task_id']" position="after">
                    <field name="jobposition_id"/>
                </xpath>
            </field>
        </record>


        <record id="view_timesheet_tree_grouped" model="ir.ui.view">
            <field name="name">account.analytic.line.tree.grouped</field>
            <field name="model">account.analytic.line</field>
            <field name="priority">1</field>
            <field name="arch" type="xml">
                <list string="Timesheets" default_group_by="task_id,jobposition_id,employee_id">
                    <field name="project_id" invisible="1"/>
                    <field name="task_id"/>
                    <field name="jobposition_id"/>
                    <field name="employee_id"/>
                    <field name="date"/>
                    <field name="name"/>
                    <field name="unit_amount" sum="Total Time" string="Time Spent"/>
                </list>
            </field>
        </record>


